{
  "name": "scrapper-front",
  "private": true,
  "version": "1.9.0",
  "scripts": {
    "====COMMANDS====": "",
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "eslint-fix": "eslint ./src --fix && tsc --noEmit",
    "generate": "graphql-codegen --config ./codegen-schema.yml && graphql-codegen",
    "====HOOKS====": "",
    "create-runtime-env-dev": "chmod +x ./env.sh && ./env.sh development && mkdir -p ./public/runtime/ && cp env-config.js ./public/runtime/",
    "create-runtime-env-prod": "chmod +x ./env.sh && ./env.sh production && mkdir -p ./public/runtime/ && cp env-config.js ./public/runtime/",
    "prestart": "yarn install",
    "====PROJECT_QA====": "",
    "bundle-visualizer": "vite-bundle-visualizer",
    "====UNIT_TESTS====": "",
    "test": "vitest watch",
    "test:no-watch": "vitest run",
    "test:ui": "vitest --ui watch",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=default --reporter=junit --outputFile.junit=./coverage/junit.xml",
    "sync-tokens": "yarn figma-tokens-transformer transform && node ./tokens/tokensToCSSVariables.js"
  },
  "dependencies": {
    "@ant-design/colors": "~7.0.0",
    "@ant-design/compatible": "^5.1.1",
    "@ant-design/icons": "~5.1.2",
    "@ant-design/pro-components": "^2.4.16",
    "@apollo/client": "~3.7.14",
    "@codexteam/ajax": "~4.2.0",
    "@graphql-codegen/schema-ast": "~4.0.0",
    "@groupher/editor-collapse": "~0.1.7",
    "@react-hook/throttle": "~2.2.0",
    "ahooks": "~3.7.7",
    "antd": "~5.5.1",
    "antd-mask-input": "~2.0.7",
    "apollo": "~2.34.0",
    "apollo-upload-client": "~17.0.0",
    "async": "~3.2.4",
    "autoprefixer": "~10.4.14",
    "axios": "~1.4.0",
    "c8": "~7.13.0",
    "classnames": "~2.3.2",
    "cross-fetch": "^3.1.6",
    "dayjs": "^1.11.7",
    "fp-ts": "~2.16.0",
    "fscreen": "^1.0.2",
    "generate-password": "^1.7.0",
    "jsdom": "~22.0.0",
    "lodash": "~4.17.21",
    "mobx": "~6.9.0",
    "mobx-react-lite": "~3.4.3",
    "mobx-state-tree": "~5.1.8",
    "mock-apollo-client": "^1.2.1",
    "moment": "~2.29.4",
    "moment-timezone": "~0.5.43",
    "mst-async-task": "~1.0.4",
    "mst-middlewares": "^5.1.6",
    "mst-persist": "~0.1.3",
    "node-sass": "~9.0.0",
    "postcss": "~8.4.23",
    "postcss-preset-env": "~8.4.1",
    "react": "~18.2.0",
    "react-beautiful-dnd": "~13.1.1",
    "react-dom": "~18.2.0",
    "react-ioc": "~1.0.0",
    "react-json-view": "~1.21.3",
    "react-router": "~6.2.2",
    "react-router-dom": "~6.2.2",
    "reflect-metadata": "~0.1.13",
    "socket.io-client": "~4.6.1",
    "tsc": "~2.0.4",
    "tsconfig-paths-webpack-plugin": "~4.0.1",
    "typedi": "~0.10.0",
    "typescript": "~5.0.4",
    "url-search-params-polyfill": "~8.1.1",
    "uuid": "~9.0.0",
    "video.js": "~7.18.1",
    "videojs-contrib-quality-levels": "~2.1.0",
    "videojs-hls-quality-selector": "~1.1.4",
    "vite": "~4.3.9",
    "vitest": "~0.31.1",
    "web-vitals": "~3.3.1",
    "zod": "~3.21.4",
    "xlsx": "^0.18.5",
    "file-saver": "^2.0.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:react-perf/recommended"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "chrome >= 65"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "engines": {
    "node": ">=16.x.x <=16.x.x",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "~4.0.0",
