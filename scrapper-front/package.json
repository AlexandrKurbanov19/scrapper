    "@graphql-codegen/typescript-document-nodes": "~4.0.0",
    "@graphql-codegen/typescript-graphql-files-modules": "~2.2.1",
    "@graphql-codegen/typescript-operations": "~4.0.0",
    "@graphql-codegen/typescript-react-apollo": "~3.3.7",
    "@types/async": "~3.2.20",
    "@types/graphql": "~14.5.0",
    "@types/lodash": "~4.14.195",
    "@types/node": "~20.2.4",
    "@types/react": "~18.2.7",
    "@types/react-beautiful-dnd": "~13.1.4",
    "@types/react-dom": "~18.2.4",
    "@types/react-router-dom": "~5.3.3",
    "@types/uuid": "~9.0.1",
    "@types/video.js": "~7.3.39",
    "@types/videojs-contrib-quality-levels": "~2.0.1",
    "@types/videojs-hls-quality-selector": "~1.1.0",
    "@typescript-eslint/eslint-plugin": "~5.59.7",
    "@typescript-eslint/parser": "~5.59.7",
    "@vitejs/plugin-react": "~4.0.0",
    "@vitest/coverage-c8": "~0.31.1",
    "@vitest/coverage-istanbul": "~0.31.1",
    "@vitest/ui": "~0.31.1",
    "babel-plugin-named-exports-order": "~0.0.2",
    "eslint": "~8.41.0",
    "eslint-config-airbnb": "~19.0.4",
    "eslint-config-airbnb-typescript": "~17.0.0",
    "eslint-plugin-graphql": "~4.0.0",
    "eslint-plugin-import": "~2.27.5",
    "eslint-plugin-jest": "~27.2.1",
    "eslint-plugin-jsx-a11y": "~6.7.1",
    "eslint-plugin-react": "~7.32.2",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-react-perf": "~3.3.1",
    "eslint-plugin-storybook": "~0.6.12",
    "eslint-plugin-tsc": "~2.0.0",
    "eslint-plugin-vitest": "~0.2.3",
    "figma-tokens-transformer": "^1.1.3",
    "precss": "~4.0.0",
    "prop-types": "15.8.1",
    "remotedev": "~0.2.9",
    "sass": "~1.62.1",
    "tailwindcss": "~3.3.2",
    "ts-deep-extract-types": "~1.0.1",
    "vite-bundle-visualizer": "~0.7.0",
    "vite-plugin-svgr": "~3.2.0",
    "vite-tsconfig-paths": "~4.2.0",
    "vitest-mock-extended": "~1.1.3"
  }
}
